pipeline{

	agent any
	tools{
		maven 'maven3'
	}

	options {
  		buildDiscarder logRotator(daysToKeepStr: '3', numToKeepStr: '2')
	}

	stages{
		
		stage('Code Checkout'){
			steps{
				git branch: 'main', url: 'https://github.com/keshav1239/jenkins-jobs'
			}
		}
		
		stage('Build'){
			steps{
				sh script: 'mvn clean install'
			}
		}
		
		
	
	}

}
